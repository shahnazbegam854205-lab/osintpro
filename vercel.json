{
  "version": 2,
  "builds": [
    { "src": "api/search.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "alluser.html", "use": "@vercel/static" },
    { "src": "autoplaysong.mp3", "use": "@vercel/static" },
    { "src": "password-change.html", "use": "@vercel/static" },
    { "src": "buy.html", "use": "@vercel/static" },
    { "src": "guid.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/alluser", "dest": "/alluser.html" },
    { "src": "/autoplaysong.mp3", "dest": "/autoplaysong.mp3" },
    { "src": "/password-change.html", "dest": "/password-change.html" },
    { "src": "/buy", "dest": "/buy.html" },
    { "src": "/src/buy", "dest": "/buy.html" },
    { "src": "/guid", "dest": "/guid.html" },
    { "src": "/src/guid", "dest": "/guid.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
